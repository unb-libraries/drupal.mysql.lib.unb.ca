version: '3.6'

services:
  drupal.mysql.lib.unb.ca:
    build:
      context: .
    command: mysqld --skip-name-resolve --max_allowed_packet=64M
    env_file:
      - ./env/mysql.env
    restart: always
    container_name: drupal.mysql.lib.unb.ca
    expose:
      - "3306"
    ports:
      - "3307:3306"
